> ## function for printIndexing indices
> printIndices <- function( what, ... ) {
+    for( i in c( "Laspeyres", "Paasche", "Fisher" ) ) {
+       cat( "\n", i, "\n" )
+       if( what == "p" ) {
+          index <- priceIndex( ..., method = i )
+       } else {
+          index <- quantityIndex( ..., method = i )
+       }
+       print( index )
+    }
+ }
