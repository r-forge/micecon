> library( "miscTools" )
> 
> 
> ## matrix
> m <- matrix( 1:24, nrow = 6, ncol = 4 )
> 
> cm1 <- colMedians( m )
> print( cm1 )
[1]  3.5  9.5 15.5 21.5
> 
> rm1 <- rowMedians( m )
> print( rm1 )
[1] 10 11 12 13 14 15
> 
> all.equal( cm1, rowMedians( t( m ) ) )
[1] TRUE
> all.equal( rm1, colMedians( t( m ) ) )
[1] TRUE
> 
